}
QLabel { color: #2B2F33; }
QLabel#titleLabel { font: 700 15pt &quot;Microsoft YaHei&quot;; }
QLineEdit, QPlainTextEdit, QTextEdit, QComboBox, QSpinBox, QDoubleSpinBox {
    background-color: #FFFFFF;
    border: 1px solid #7F8B96;
    border-radius: 6px;
    padding: 4px 8px;
    color: #2B2F33;
}
QLineEdit:focus, QPlainTextEdit:focus, QTextEdit:focus, QComboBox:focus, QSpinBox:focus, QDoubleSpinBox:focus {
    border: 1px solid #2F7D66;
}
QComboBox::drop-down {
    border-left: 1px solid #7F8B96;
    width: 24px;
}
QComboBox::down-arrow {
    image: url(:/new/prefix1/style/QSS_IMG/go-down-symbolic.symbolic.png);
    width: 12px;
    height: 12px;
}
QComboBox::down-arrow:on {
    image: url(:/new/prefix1/style/QSS_IMG/go-up-symbolic.symbolic.png);
}
QPushButton {
    background-color: #F7F9FB;
    color: #1F2328;
    border: 1px solid #7F8B96;
    border-radius: 8px;
    padding: 6px 14px;
}
QPushButton:hover { background-color: #EEF2F6; }
QPushButton:pressed {
    background-color: #E1E7ED;
    padding-top: 8px;
    padding-bottom: 6px;
}
QPushButton#exportPresetButton, QPushButton#savePresetButton {
    background-color: #1FA87A;
    color: #FFFFFF;
    border: 1px solid #1A8F68;
}
QPushButton#exportPresetButton:hover, QPushButton#savePresetButton:hover { background-color: #19966B; }
QPushButton#exportPresetButton:pressed, QPushButton#savePresetButton:pressed { background-color: #157E5A; }
QTabWidget::pane {
    border: 1px solid #CBD5DF;
    border-radius: 8px;
    background: #FFFFFF;
    top: -1px;
}
QTabBar::tab {
    background: #ECF0F3;
    color: #2B2F33;
    border: 1px solid #CBD5DF;
    padding: 8px 22px;
    min-width: 140px;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    margin-right: 2px;
}
QTabBar::tab:selected {
    background: #FFFFFF;
    border-bottom-color: #FFFFFF;
    font-weight: 600;
}
QListWidget {
    background-color: #FFFFFF;
    border: 1px solid #CBD5DF;
    border-radius: 6px;
    padding: 4px;
}
QListWidget::item {
    padding: 6px 8px;
    border-radius: 4px;
}
QListWidget::item:selected {
    background-color: #DDEFE9;
    color: #1E2A24;
}
QCheckBox { color: #2B2F33; }
QGroupBox {
    border: 1px solid #CBD5DF;
    border-radius: 8px;
    margin-top: 10px;
    padding-top: 12px;
}
QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 4px;
    color: #4A4F55;
    font-weight: 600;
}
QSplitter::handle {
    background-color: #D4DBE3;
    width: 2px;
}</string>
